{
   "name": "n8n-nodes-tally",
	"version": "1.0.0",
	"description": "n8n community node for Tally.so forms and submissions",
	"keywords": [
		"n8n-community-node-package",
		"tally",
		"forms",
		"submissions",
		"n8n",
		"workflow"
	],
	"license": "MIT",
	"homepage": "https://github.com/brianmoney/n8n-nodes-tally",
	"author": {
		"name": "Brian Money",
		"email": "brianmoney@users.noreply.github.com"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/brianmoney/n8n-nodes-tally.git"
	},
	"main": "index.js",
	"scripts": {
		"build": "tsc && gulp build:icons",
		"dev": "tsc --watch",
		"format": "prettier nodes credentials --write",
		"lint": "eslint nodes credentials package.json",
		"lintfix": "eslint nodes credentials package.json --fix",
		"prepublishOnly": "npm run build"
	},
   "files": [
	   "dist",
	   "index.js"
   ],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"credentials": [
			"dist/credentials/TallySoApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/TallySo/TallySo.node.js"
		]
	},
	"devDependencies": {
		"@typescript-eslint/parser": "~5.45.0",
		"eslint-plugin-n8n-nodes-base": "~1.11.0",
		"gulp": "^4.0.2",
		"n8n-workflow": "*",
		"prettier": "^2.7.1",
		"typescript": "~4.8.4"
	},
	"dependencies": {}
}
